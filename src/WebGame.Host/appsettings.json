{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "TokenServiceOptions": {
    "SecretKey": "your-secret-key-at-least-32-chars",
    "Issuer": "your-issuer",
    "Audience": "your-audience",
    "AccessTokenExpirationMinutes": 10,
    "RefreshTokenExpirationDays": 1
  }
}